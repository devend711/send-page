{
  "manifest_version": 2,

  "name": "Send-Page",
  "description": "Send a page to your phone in one click",
  "version": "1.0",
  "options_page": "options.html",

  "permissions": [
    "https://secure.flickr.com/",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "cat.png",
    "default_popup": "popup.html"
  }
}